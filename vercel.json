{
    "version": 2,
    "builds": [
        {
            "src": "dist/src/main.js",
            "use": "@vercel/node",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/src/main.js",
            "methods": ["GET", "POST", "PATCH", "DELETE"]
        }
    ]
}
